let configpath='$HOME/.config/cvim/.cvimrc'
set localconfig

let searchengine reddit = ["https://www.reddit.com/", "https://www.reddit.com/search/?q="]
let searchengine youtube = ["https://www.youtube.com/", "https://www.youtube.com/results?search_query="]
let searchengine github = ["https://www.github.com/", "https://github.com/search?q="]
" let mapleader=" "
let defaultengine="duckduckgo"
let completionengine=["duckduckgo", "wikipedia", "youtube"]

imap <C-o> editWithVim
map # lastUsedTab
map a :tabnew duckduckgo<CR>

let searchalias g = "google"
let searchalias y = "youtube"
let searchalias d = "duckduckgo"
let searchalias w = "wikipedia"
let searchalias r = "reddit"
let searchalias gh = "github"

map s :open<space>d<space>
map w :open<space>w<space>
